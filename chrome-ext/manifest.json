{
  "manifest_version": 2,
  "name": "Jenkins Chrome Extension",
  "short_name": "JenkinsExt",
  "description": "Jenkins Chrome Extension",
  "version": "1.0.2",
  "browser_action": {
    "default_title": "Jenkins Chrome Extension",
    "default_icon": "img/jenkins-icon-128px.png",
    "default_popup": "popup/jenkins-ext-popup.html"    
  },
  "icons": {
    "48": "img/jenkins-icon-48px.png",
    "96": "img/jenkins-icon-96px.png",
    "128": "img/jenkins-icon-128px.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://mydtbld0101.hpeswlab.net:8888/jenkins/job/*",
        "http://mydtbld0101.hpeswlab.net:8888/jenkins/view/*/job/*"
      ],
      "css" :[
        "content/jenkins-ext-content.css"
	  ],
	  "js" :[
        "content/jenkins-ext-content.js"
	  ]
    } 
  ]
}
